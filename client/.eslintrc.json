{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent": [
      true,
      4
    ],
    "react/jsx-indent-props": [
      true,
      4
    ],
    "react/jsx-one-expression-per-line": [
      0
    ],
      "no-underscore-dangle": [
      0
    ]
  }
}
